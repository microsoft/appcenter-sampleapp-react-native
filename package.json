{
  "name": "AppCenterSample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-community/cli-platform-android": "^3.0.3",
    "@types/react": "^16.9.13",
    "@types/react-native": "^0.60.5",
    "@types/react-navigation": "^1.5.15",
    "appcenter": "1.13.0",
    "appcenter-analytics": "1.13.0",
    "appcenter-crashes": "1.13.0",
    "react": "^16.12.0",
    "react-native": "^0.61.5",
    "react-native-code-push": "^5.7.0",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-navigation": "^1.6.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/react-test-renderer": "^16.9.1",
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.8.0",
    "eslint": "^6.1.0",
    "jest": "24.8.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2",
    "metro-react-native-babel-preset": "^0.56.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-[a-z|-]*)/)"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "setupFiles": [
      "./node_modules/appcenter/test/AppCenterMock.js",
      "./node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "./node_modules/appcenter-crashes/test/AppCenterCrashesMock.js"
    ]
  }
}
